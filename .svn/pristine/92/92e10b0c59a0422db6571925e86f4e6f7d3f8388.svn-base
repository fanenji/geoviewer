<!DOCTYPE HTML>

<html> 
    <head> 
        <title>Esempio Viewport</title> 

        <link rel="stylesheet" type="text/css" href="/cartowebNet2/lib/ExtJs/resources/css/ext-all.css" />
        <link rel="stylesheet" type="text/css" href="/cartowebNet2/lib/ExtJs/resources/css/xtheme-light-green.css" />
        <link rel="stylesheet" type="text/css" href="/cartowebNet2/stili/default/base.css" />
		<!-- librerie js -->
        <script src="http://maps.google.com/maps/api/js?sensor=false&v=3.2&language=it"></script>
        <script src="/cartowebNet2/lib/OL/OpenLayers.js" type="text/javascript"></script>
		<!-- proj4js -->
		<script src='/cartowebNet2/lib/proj4js/lib/proj4js-min.js'></script>
		<!-- jQuery -->
        <script src="/cartowebNet2/lib/jQuery/jquery-1.7.min.js"></script>
		<!-- ExtJs -->
        <script src="/cartowebNet2/lib/ExtJs/ext-all-full.js"></script>
		<!-- GeoExt -->
		<script src="/cartowebNet2/lib/GeoExt/script/GeoExt.js"></script>
		<!-- CWN2 -->
		<script src="/cartowebNet2/lib/Cwn2/cwn2-debug.js"></script>



	</head>
	<body>

		<!-- codice js della applicazione -->
		<script>
			
			Ext.onReady(function() {
    
			Ext.QuickTips.init();
		
			// imposto la url del servizio di configurazione
			var idApp = CWN2.Util.getUrlParam('idApp');
			var idMap = CWN2.Util.getUrlParam('idMap');
			var idLayers = CWN2.Util.getUrlParam('idLayers');
			var debug = CWN2.Util.getUrlParam('debug');
//            var configUrl = "http://srvcarto.regione.liguria.it/cartowebnet2/services/Configuration/getConfig.ashx?idApp=" + idApp + "&idMap=" + idMap + "&idLayers=" + idLayers + "&debug=" + debug;
            var configUrl = "http://parodis-dts-pc.datasiel.net/cartowebnet2/services/Configuration/getConfig.ashx?idApp=" + idApp + "&idMap=" + idMap + "&idLayers=" + idLayers + "&debug=" + debug;

			// carico la mappa
			var initOptions = {
				appConfig: configUrl,
				divID: null,
				callBack: null,
                proxy: "/CartoWebNet2/services/proxy/proxy.ashx?url=",
                language:"en",
				debug:true
			};
            var app = new CWN2.Application(initOptions);

		});

		</script> 

	</body>
</html>
