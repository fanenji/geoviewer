BODY
{
    MARGIN-TOP: 0px;
    FONT-SIZE: 11px;
    MARGIN-RIGHT: 5px;
    FONT-FAMILY: verdana,arial,helvetica,sans-serif;
    BACKGROUND-COLOR: #99cccc;

}
TD
{
    FONT-SIZE: 10px;
    MARGIN-RIGHT: 20px;
    FONT-FAMILY: Verdana,arial,helvetica;
    TEXT-ALIGN: justify
}
TD.centro
{
    FONT-SIZE: 12px;
    MARGIN-RIGHT: 20px;
    FONT-FAMILY: Verdana,arial,helvetica;
    BACKGROUND-COLOR: #dddddd;
    TEXT-ALIGN: center;
}
H1
{
    FONT-WEIGHT: bold;
    FONT-SIZE: 14px;
    FONT-FAMILY: verdana,arial,helvetica,sans-serif;
    TEXT-ALIGN: center
}

A
{
    COLOR: teal
}

A:link
{
    COLOR: #FF0000;
}
A:visited
{
    COLOR: #990000;
}
A:active
{
    TEXT-DECORATION: none;
}
A:hover
{
    COLOR: #FFFFFF;
    TEXT-DECORATION: underline;
}
.Stile5
{
    FONT-WEIGHT: bold;
    COLOR: #ffffff;
    TEXT-ALIGN: right
}
A.Stile5:link
{
    COLOR: #ffffff;
    TEXT-DECORATION: none
}
A.Stile5:visited
{
    COLOR: #ffffff;
    TEXT-DECORATION: none
}
A.Stile5:active
{
    COLOR: #ffffff;
    TEXT-DECORATION: none
}
A.Stile5:hover
{
    COLOR: #ffffff;
    TEXT-DECORATION: underline
}

